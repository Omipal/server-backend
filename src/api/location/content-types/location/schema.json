{
  "kind": "collectionType",
  "collectionName": "locations",
  "info": {
    "singularName": "location",
    "pluralName": "locations",
    "displayName": "Location"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "Corporate",
        "Distribution",
        "Manufacturing"
      ]
    },
    "addressLine": {
      "type": "string"
    },
    "city": {
      "type": "string"
    },
    "state": {
      "type": "string"
    },
    "zip": {
      "type": "string"
    },
    "country": {
      "type": "string"
    },
    "phone": {
      "type": "string"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images"
      ]
    },
    "latitude": {
      "type": "decimal",
      "min": -90,
      "max": 90
    },
    "longitude": {
      "type": "decimal",
      "min": -180,
      "max": 180
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "isActive": {
      "type": "boolean",
      "default": false
    }
  }
}
